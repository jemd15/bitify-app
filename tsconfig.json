{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@modules/*": ["src/modules/*"],
      "@lib/*": ["src/lib/*"],
      "@store/*": ["src/store/*"],
      "@shared/*": ["src/shared/*"],
      "@app/*": ["src/app/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", "src/shared/types/env.d.ts"]
}
